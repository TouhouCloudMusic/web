{
  "name": "@touhouclouddb/database",
  "main": "index.ts",
  "devDependencies": {
    "@edgedb/generate": "^0.5.4",
    "@types/bun": "^1.1.6",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.6.0-dev.20240728",
    "edgedb": "^1.5.8"
  },
  "exports": {
    ".": "./index.ts",
    "./interfaces": "./interfaces.ts",
    "./*": "./edgeql-js/*"
  },
  "scripts": {
    "generate": "npx @edgedb/generate interfaces --file interfaces.ts && npx @edgedb/generate edgeql-js --target esm --output-dir ./edgeql-js",
    "migration": "edgedb migration create",
    "migrate": "edgedb migrate",
    "all": "bun run migration && bun run migrate && bun run generate",
    "format": "prettier --write .",
    "check": "prettier --check .",
    "pre-push": "bun run check"
  },
  "type": "module"
}
