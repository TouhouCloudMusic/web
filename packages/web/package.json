  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "check": "prettier --check ./src",
    "lint::es": "eslint --cache",
    "lint::oxc": "bunx oxlint -c oxlintrc.json --import-plugin --disable-react-plugin --jsx-a11y-plugin --promise-plugin",
    "lint": "bun run lint::oxc && bun run lint::es",
    "fix": "bun run lint::oxc --fix && bun run lint::es --fix",
    "test::vite": "bun vitest run",
    "test::bun": "bun test ./**/*.buntest.ts",
    "test": "bun run test::dom && bun run test::bun",
    "pre-push": "bun run check && bun run lint"
  },
  "license": "MIT",
