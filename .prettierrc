{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"overrides": [
		{
			"files": ["*.json"],
			"options": {
				"trailingComma": "none"
			}
		},
		{
			"files": ["*.yaml", "*.yml"],
			"options": {
				"useTabs": false,
				"tabWidth": 2
			}
		}
	],
	"printWidth": 80,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"rangeStart": 0,
	"requirePragma": false,
	"semi": false,
	"singleAttributePerLine": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true,
	"vueIndentScriptAndStyle": false
}
